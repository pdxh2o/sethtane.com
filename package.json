{
  "name": "www.sethtane.com",
  "version": "1.0.0",
  "description": "www.sethtane.com",
  "scripts": {
    "dev": ". ./env.sh && dev",
    "build": ". ./env.sh && build-css && build-js && [ \"$NODE_ENV\" = \"production\" ] && npm run minify",
    "minify": "cp share/build.js share/build.debug.js; uglifyjs share/build.debug.js > share/build.js",
    "start": "npm run build && app-server",
    "migrate": ". ./env.sh && node bin/migrate.js",
    "migrate-field": ". ./env.sh && node bin/migrate-field.js"
  },
  "dependencies": {
    "build-css": "jessetane/build-css#1.0.6",
    "build-js": "jessetane/build-js#1.0.1",
    "hyperglue2": "^2.0.0",
    "marked": "^0.3.5",
    "uglify-js": "^2.6.1",
    "uri-router": "jessetane/uri-router#0.0.5"
  },
  "devDependencies": {
    "dev": "jessetane/dev",
    "queue": "^3.1.0"
  },
  "repository": "https://github.com/pdxh2o/sethtane.com",
  "author": "jesse.tane@gmail.com"
}
